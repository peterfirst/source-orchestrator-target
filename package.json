{
  "name": "backend-engineer",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "install:production": "npm install",
    "package:nodemodules": "mkdir -p dist/nodejs && cd dist/nodejs && ln -sf ../../node_modules && cd ../ && zip -rq node_modules.zip nodejs/",
    "package:dispatcher": "cd dist && zip -rq dispatcher.zip dispatcher.js",
    "package:processor": "cd dist && zip -rq processor.zip processor.js",
    "package:health": "cd dist && zip -rq health.zip health.js",
    "package:production": "sh package-production.sh",
    "prebuild": "npm run clean && npm run install:production",
    "build": "node build.js",
    "postbuild": "npm run package:production",
    "test": "jest",
    "jest:watch": "jest --watch",
    "prettier": "prettier --write src/**/*.*",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.782.0",
    "@aws-sdk/client-sqs": "^3.777.0",
    "@aws-sdk/util-dynamodb": "^3.782.0",
    "aws-lambda": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.17",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "esbuild": "^0.25.2",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
